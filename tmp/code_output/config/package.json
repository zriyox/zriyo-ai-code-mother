{
  "name": "vue3-vite-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.vue --quiet"
  },
  "dependencies": {
    "@vueuse/motion": "^3.0.3",
    "magic-string": "^0.30.21",
    "vue": "^3.4.30",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "unplugin-auto-import": "^20.3.0",
    "vite": "^5.4.10",
    "vite-plugin-html": "^3.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off",
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  }
}
